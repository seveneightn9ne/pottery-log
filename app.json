{
  "expo": {
    "sdkVersion": "33.0.0",
    "name": "Pottery Log",
    "icon": "./assets/launcher_512.png",
    "slug": "pottery-log",
    "version": "2.33.0",
    "privacy": "public",
    "assetBundlePatterns": ["./assets/*.png", "./assets/*.ttf"],
    "android": {
      "package": "com.jesskenney.pottery_log",
      "permissions": [
        "CAMERA",
        "MANAGE_DOCUMENTS",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 7
    },
    "ios": {
      "bundleIdentifier": "com.jesskenney.potterylog",
      "buildNumber": "1.0.3",
      "icon": "./assets/launcher_ios_1024.png",
      "usesIcloudStorage": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Pottery Log to use the camera to take photos of your pots.",
        "NSPhotoLibraryAddUsageDescription": "Allow Pottery Log to store your pot photos.",
        "NSPhotoLibraryUsageDescription": "Allow Pottery Log to use photos from your Camera Roll."
      }
    },
    "primaryColor": "#4caf50",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "jess-kenney",
            "project": "pottery-log"
          }
        }
      ]
    }
  }
}

{
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*"
    ]
  },
  "rules": {
    "quotemark": [true, "single", "jsx-double", "avoid-escape", "avoid-template"]
  }
}
